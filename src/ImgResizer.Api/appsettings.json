{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "ImgResizer": "Debug"
    }
  },
  "AllowedHosts": "*",
  "ImageResize": {
    "InputDirectory": "C:\\images\\input",
    "OutputDirectory": "C:\\images\\output",
    "TargetSize": {
      "Width": 512,
      "Height": 512
    },
    "AllowedExtensions": [".jpg", ".jpeg", ".png", ".gif", ".bmp"],
    "MaxFileSize": 52428800,
    "PaddingColor": {
      "R": 0,
      "G": 0,
      "B": 0,
      "A": 255
    },
    "ImageQuality": {
      "JpegQuality": 90,
      "PngCompressionLevel": 6
    }
  }
}
